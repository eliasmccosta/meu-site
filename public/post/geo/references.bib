@article{Santos2016,
abstract = {Different parent materials participate in the formation of soils in the$\backslash$nhilly landscape of ``Mar de Morros{\{}''{\}} in the Atlantic Forest$\backslash$nenvironment. Those derived from mafic igneous rock (gabbro) frequently$\backslash$nshow erosion problems because of land use, which is aggravated by the$\backslash$nmountainous relief and soil attributes. This study evaluated the main$\backslash$npedogenic processes of soils formed from mafic igneous rock (gabbro) in$\backslash$na toposequence in Pinheiral (RJ) by characterizing physical, chemical,$\backslash$nmineralogical and micromorphological attributes. The profiles are$\backslash$nlocated at different sections in the toposequence: summit (P1), shoulder$\backslash$n(P2), backslope (P3) and footslope (P4). They were classified according$\backslash$nto the Brazilian System of Soil Classification (SiBCS) and correlated to$\backslash$nSoil Taxonomy. The soil morphology of profiles P2, P3 and P4 is$\backslash$nexpressed by a brownish-red color, blocky structure with high to$\backslash$nmoderate development, clay films and clay loam to clay texture, with a$\backslash$ntextural B horizon. P1 shows less development, with a shallow profile$\backslash$nand the sequence of horizons A-C-Cr. The soils have a slightly low$\backslash$ndegree of weathering, identified by the presence of pyroxenes and$\backslash$nfeldspars in the sand fraction and montorillonite in the clay fraction;$\backslash$nthe sum of bases is from 15 to 24 cmol(c) kg(-1); and cation exchange$\backslash$ncapacity (CEC) is from 12 to 22 cmol(c) kg(-1). A significant presence$\backslash$nof clay skins was observed in the field and was confirmed by thin$\backslash$nsection analysis, which showed features such as argillans,$\backslash$nferriargillans and iron nodules. The soil profile at the summit (P1) was$\backslash$nclassified as Neossolo Regolitico Ortico (Typic Udorthents), and the$\backslash$nother profiles as Chernossolo Argiluvicos Orticos (Typic Argiudolls).},
author = {Santos, Adailde do Carmo and Pereira, Marcos Gervasio and dos Anjos, L{\'{u}}cia Helena Cunha and Bernini, Thiago Andrade and Cooper, Miguel},
doi = {10.1590/18069657rbcs20150056},
file = {:C$\backslash$:/Users/em43049/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Santos et al. - 2016 - Genesis of soils formed from mafic igneous rock in the Atlantic forest environment.pdf:pdf},
issn = {18069657},
journal = {Revista Brasileira de Ciencia do Solo},
keywords = {Pedogenesis,SiBCS,Soil weathering},
pages = {1--21},
title = {{Genesis of soils formed from mafic igneous rock in the Atlantic forest environment}},
volume = {40},
year = {2016}
}
