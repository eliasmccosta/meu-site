isbn = {0016-7061},
issn = {00167061},
journal = {Geoderma},
keywords = {Conditioned Latin hypercube sampling,Digital soil mapping,Golestan,Random forests,Soil series},
pages = {97--106},
publisher = {Elsevier B.V.},
title = {{Updating soil survey maps using random forest and conditioned Latin hypercube sampling in the loess derived soils of northern Iran}},
url = {http://dx.doi.org/10.1016/j.geoderma.2014.04.036},
volume = {232-234},
