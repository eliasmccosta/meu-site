@article{Feoli2016,
abstract = {Area-pedodiversity, area–vegetation diversity, area-bioclimatic belts, diversity and pedodiversity-vegetation diversity-bioclimatic diversity relationships were analyzed for the most arid region of Western Europe (SE Spain). The study is novel in that it analyzed these relationships using as operational geographic units (OGUs) the set of polygons belonging to the same character state (i.e., typology) that defines a zone in a given thematic map. We considered three thematic maps: 1) the map of soil associations (SMU) with 303 different soil associations (i.e., 303 areas or zones); 2) the map of potential vegetation (PNV) with 40 vegetation series or zones and 3) the map of bioclimatic belts (BB) with 7 bioclimatic zones. Using GIS tools, we analyzed the contents (richness) of soil mapping units (polygons) with regard to vegetation series and bioclimatic belts as well as the contents of vegetation series and bioclimatic belts with regard to SMU. The results indicate that the relationships between the area and the richness of zones follow a power law, the “fingerprint of fractal geometry”, irrespective of the way the area has been defined and their relative magnitude (the areas defined by SMU are the smallest, while the areas defined by bioclimatic belts are the largest). The results also indicate a significant correlation between the $\alpha$ diversities of the zones and between their $\beta$ diversities. We conclude that the methods used to measure such correlations, based on $\alpha$ and $\beta$ diversities are useful to investigate and quantify the relationships between the pedosphere, vegetation and climate.},
author = {Feoli, Enrico and P{\'{e}}rez-G{\'{o}}mez, Rufino and Oyonarte, Cecilio and Ib{\'{a}}{\~{n}}ez, Juan J.},
doi = {10.1016/j.geoderma.2016.09.011},
file = {:C$\backslash$:/Users/em43049/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Feoli et al. - 2016 - Using spatial data mining to analyze area-diversity patterns among soil, vegetation, and climate A case study from.pdf:pdf},
issn = {00167061},
journal = {Geoderma},
pages = {1--6},
title = {{Using spatial data mining to analyze area-diversity patterns among soil, vegetation, and climate: A case study from Almer{\'{i}}a, Spain}},
