issn = {14154366},
journal = {Revista Brasileira de Engenharia Agr{\'{i}}cola e Ambiental},
keywords = {aster,remote sensing,supervised classification},
number = {3},
pages = {319--327},
title = {{Utiliza{\c{c}}{\~{a}}o de redes neurais artificiais na classifica{\c{c}}{\~{a}}o de n{\'{i}}veis de degrada{\c{c}}{\~{a}}o em pastagens}},
