issn = {18069657},
journal = {Revista Brasileira de Ciencia do Solo},
keywords = {GIS,Model,Prediction,Soil map},
number = {4},
pages = {960--967},
title = {{Sele{\c{c}}{\~{a}}o de densidade de amostragem com base em dados de {\'{a}}reas j{\'{a}} mapeadas para treinamento de modelos de {\'{a}}rvore de decis{\~{a}}o no mapeamento digital de solos}},
