pages = {141--154},
publisher = {Elsevier B.V.},
title = {{Predictive soil parent material mapping at a regional-scale: A Random Forest approach}},
url = {http://dx.doi.org/10.1016/j.geoderma.2013.09.016},
volume = {214-215},
year = {2014}
