@article{Wechsler2006,
abstract = {Digital elevation models (DEMs) are representations of topography with inherent errors that constitute uncertainty. DEM data are often used in analyses without quantifying the effects of these errors. This paper describes a Monte Carlo methodology for evaluation of the effects of uncertainty on elevation and derived topographic parameters. Four methods for representing DEM uncertainty that utilize meta- data and spatial characteristics of a DEM are presented. Seven statistics derived from simulation results were used to quantify the effect of DEM error. When uncertainty was quantified by the average relative absolute difference, ele- vation did not deviate. The range of deviation across the four methods for slope was 5 to 8 percent, 460 to 950 percent for derived catchment areas and 4 to 9 percent for the topographic index. This research demonstrates how application of this methodology can address DEM uncer- tainty, contributing to more responsible use of elevation and derived topographic parameters, and ultimately results obtained from their use.},
author = {Wechsler, Suzanne P. and Kroll, Charles N.},
doi = {10.14358/PERS.72.9.1081},
file = {:C$\backslash$:/Users/em43049/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wechsler, Kroll - 2006 - Quantifying DEM uncertainty and its effect on topographic parameters.pdf:pdf},
