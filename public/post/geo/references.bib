file = {:C$\backslash$:/Users/em43049/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gimona, Van Der Horst - 2007 - Mapping hotspots of multiple landscape functions A case study on farmland afforestation in Scotland.pdf:pdf},
isbn = {0921-2973},
issn = {09212973},
journal = {Landscape Ecology},
keywords = {Biodiversity,Farm woodlands,GIS,Landscape,Multifunctionality,Recreation,Spatial targeting,Visual amenity},
number = {8},
pages = {1255--1264},
pmid = {26147500},
title = {{Mapping hotspots of multiple landscape functions: A case study on farmland afforestation in Scotland}},
