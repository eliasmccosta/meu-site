journal = {Boletim de Geografia},
keywords = {Impacts Management,Itatiaia National Park,Manejo de Impactos,Parque Nacional do Itatiaia,Protected Areas,Public Use,Unidades de Conserva{\c{c}}{\~{a}}o,Uso P{\'{u}}blico,ecoturismo},
