journal = {Pesquisa Agropecuaria Brasileira},
number = {9},
pages = {1359--1370},
title = {{Mapeamento digital de areia, argila e carbono org{\^{a}}nico por modelos Random Forest sob diferentes resolu{\c{c}}{\~{o}}es espaciais}},
